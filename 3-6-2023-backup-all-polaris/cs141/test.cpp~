#include <iostream>

#include <vector>


using namespace std;

int main(){

//this is a comment


  vector<int> better_bib(1,2,3);

  better_bib.push_back(1);
  better_bib.push_back(2);
  better_bib.push_back(3);

  int bib[2][2] ={{1,2}, {3,4}};

  for(int i=0; i<2; i++){
    for(int j=0; j<2; j++){
      
      cout<<bib[i][1]<<endl;
    }
  }

  cout<<"-----------------------------------------"<<endl;

  for(int k=0; k<better_bib.size(); k++){

    cout<<better_bib[k]<<endl;

  }



	
	return 0;
}
